{
  "name": "cypress-timeout-plugin",
  "version": "1.0.0",
  "description": "A plugin for Cypress to optimize tests with frequently changing UI elements and API requests through automatic timeout management.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "cy:open": "cypress open",
    "run-plugin": "node --loader ts-node/esm src/index.ts",
    "run-file-reader": "node --loader ts-node/esm src/read_file.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Diosa34/cypress-timeout-plugin.git"
  },
  "keywords": [
    "plugin"
  ],
  "author": "Osokina Maria Yurievna",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Diosa34/cypress-timeout-plugin/issues"
  },
  "homepage": "https://github.com/Diosa34/cypress-timeout-plugin#readme",
  "dependencies": {
    "cypress": "12.17.4"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.3",
    "ts-node": "^10.9.2",
    "typescript": "*"
  }
}
